<body>
    <div class="navbar">
        <div class="nav_leftPart">
          <div class="logo-name">
            <div class="image"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQtCEgzpYztDhYSmSVnY3-jAPri66LFXOp0HA&s"></div>
                <div class="name"><p><a href="https://www.nitap.ac.in/">राष्ट्रीय प्रौद्योगिकी संस्थान, अरुणाचल प्रदेश</a></p>
                                <p><a href="https://www.nitap.ac.in/">NATIONAL INSTITUTE OF TECHNOLOGY, ARUNACHAL PRADESH</a></p>
                </div>
          </div>
                <div class="hamburger"><i class="fa-solid fa-bars"></i></div>
                <div class="crossSign"><i class="fa-regular fa-rectangle-xmark"></i></div>
        </div>
        <div class="options">
            <div class="option"><a href="/results">Home</a></div>
            <% if(currUser && currUser.isSuperAdmin===true){%>
                <div class="option"><a href="/admin/ListOfAdmin">show-Admin's</a></div>
            <%}%>
            <%if(!currUser){%>
                <div class="option"><a href="/signup">Sign-Up</a></div>
                <div class="option"><a href="/login">Login</a></div>
            <%}else if(currUser && currUser.role==="admin"){%>
                <div class="option"><a href="/">Admin</a></div>
                <div class="option"><a href="/logout">Logout</a></div>
            <%}else if(currUser){%>
                <div class="option"><a href="/logout">Logout</a></div>
            <%}%>
        </div>
    </div>
    <script src="/Js/script.js"></script>
</body> 
